"\n"
"brug '%s --help' uden andre flag for flere syntaks\n"

#: ../pacman-g2.c:106
#, c-format
msgid "usage:  %s {-A --add} [options] <file>\n"
msgstr "brug:  %s {-A --add} [valgmuligheder] <fil>\n"

#: ../pacman-g2.c:107 ../pacman-g2.c:113 ../pacman-g2.c:125 ../pacman-g2.c:130
#: ../pacman-g2.c:145
#, c-format
msgid "options:\n"
msgstr "valgmuligheder:\n"

#: ../pacman-g2.c:108 ../pacman-g2.c:115 ../pacman-g2.c:126 ../pacman-g2.c:147
#, c-format
msgid "  -d, --nodeps        skip dependency checks\n"
msgstr "  -d, --nodeps        spring afhængigheds tjeks over\n"

#: ../pacman-g2.c:109 ../pacman-g2.c:127 ../pacman-g2.c:149
#, c-format
msgid "  -f, --force         force install, overwrite conflicting files\n"
msgstr ""
"  -f, --force         gennemtving installation, overskriv konfliktende "
"filer\n"

#: ../pacman-g2.c:110
#, c-format
msgid ""
"      --noarch        install the package, even if it is for an other arch\n"
msgstr ""

#: ../pacman-g2.c:112
#, c-format
msgid "usage:  %s {-R --remove} [options] <package>\n"
msgstr "brug:  %s {-R --remove} [valgmuligheder] <pakke>\n"

#: ../pacman-g2.c:114
#, c-format
msgid ""
"  -c, --cascade       remove packages and all packages that depend on them\n"
msgstr ""
"  -c, --cascade       fjern pakker og alle pakker der afhænger af dem\n"

#: ../pacman-g2.c:116
#, c-format
msgid "  -k, --dbonly        only remove database entry, do not remove files\n"
msgstr "  -k, --dbonly        fjern kun database post, fjern ikke filer\n"

#: ../pacman-g2.c:117
#, c-format
msgid "  -n, --nosave        remove configuration files as well\n"
msgstr "  -n, --nosave        fjern også konfigurations filerne\n"

#: ../pacman-g2.c:118
#, c-format
msgid ""
"  -s, --recursive     remove dependencies also (that won't break packages)\n"
msgstr ""
"  -s, --recursive     fjern også afhængigheder (det vil ikke ødelægge "
"pakker)\n"

#: ../pacman-g2.c:121
#, c-format
msgid "usage:  %s {-F --freshen} [options] <file>\n"
msgstr "brug:  %s {-F --freshen} [valgmuligheder] <fil>\n"

#: ../pacman-g2.c:123
#, c-format
msgid "usage:  %s {-U --upgrade} [options] <file>\n"
msgstr "brug:  %s {-U --upgrade} [valgmuligheder] <fil>\n"

#: ../pacman-g2.c:129
#, c-format
msgid "usage:  %s {-Q --query} [options] [package]\n"
msgstr "brug:  %s {-Q --query} [valgmuligheder] [pakke]\n"

#: ../pacman-g2.c:131
#, c-format
msgid "  -c, --changelog     view the changelog of a package\n"
msgstr "  -c, --changelog     se ændrings log på en pakke\n"

#: ../pacman-g2.c:132
#, c-format
msgid ""
"  -e, --orphans       list all packages that were installed as a dependency\n"
msgstr ""
"  -e, --orphans       liste alle pakker der er installeret som en "
"afhængighed\n"

#: ../pacman-g2.c:133
#, c-format
msgid "                      and are not required by any other packages\n"
msgstr "                      og er ikke krævet af nogen andre pakker\n"

#: ../pacman-g2.c:134 ../pacman-g2.c:150
#, c-format
msgid "  -g, --groups        view all members of a package group\n"
msgstr "  -g, --groups        se alle medlemmer af en pakke gruppe\n"

#: ../pacman-g2.c:135
#, c-format
msgid "  -i, --info          view package information\n"
msgstr "  -i, --info          se pakke information\n"

#: ../pacman-g2.c:136
#, c-format
msgid "  -l, --list          list the contents of the queried package\n"
msgstr "  -l, --list          liste indholdet af den forespurgte pakke\n"

#: ../pacman-g2.c:137
#, c-format
msgid ""
"  -m, --foreign       list all packages that were not found in the sync db"
"(s)\n"
msgstr ""
"  -m, --foreign       liste alle pakker der ikke var fundet i synkron "
"databasse(r)\n"

#: ../pacman-g2.c:138
#, c-format
msgid "  -o, --owns <file>   query the package that owns <file>\n"
msgstr "  -o, --owns <fil>   forespørge pakken der ejer <fil>\n"

#: ../pacman-g2.c:139
#, c-format
msgid ""
"  -p, --file          pacman-g2 will query the package file [package] "
"instead of\n"
msgstr ""
"  -p, --file          pacman-g2 vil forespørge pakke filen [pakke] i stedet "
"for\n"

#: ../pacman-g2.c:140
#, c-format
msgid "                      looking in the database\n"
msgstr "                      ser efter i databassen\n"

#: ../pacman-g2.c:141
#, c-format
msgid ""
"  -s, --search        search locally-installed packages for matching "
"strings\n"
msgstr ""
"  -s, --search        søger lokale-installerede pakker for matchende "
"strenge\n"

#: ../pacman-g2.c:142
#, c-format
msgid "  -t, --test          search problems in the local database\n"
msgstr "  -t, --test          søger efter problemer i den lokale database\n"

#: ../pacman-g2.c:144
#, c-format
msgid "usage:  %s {-S --sync} [options] [package]\n"
msgstr "brug:  %s {-S --sync} [valgmuligheder] [pakke]\n"

#: ../pacman-g2.c:146
#, c-format
msgid ""
"  -c, --clean         remove old packages from cache directory (use -cc for "
"all)\n"
msgstr ""
"  -c, --clean         fjern gamle pakker fra cache katalog (brug -cc for "
"alle)\n"

#: ../pacman-g2.c:148
#, c-format
msgid "  -e, --dependsonly   install dependencies only\n"
msgstr "  -e, --dependsonly   installer kun afhængigheder\n"

#: ../pacman-g2.c:151
#, c-format
msgid ""
"  -p, --print-uris    print out URIs for given packages and their "
"dependencies\n"
msgstr ""
"  -p, --print-uris    udskriv URIs for givende pakker og deres "
"afhængigheder\n"

#: ../pacman-g2.c:152
#, c-format
msgid "  -s, --search        search remote repositories for matching strings\n"
msgstr "  -s, --search        søg ekstern arkiv for matchende strenge\n"

#: ../pacman-g2.c:153
#, c-format
msgid "  -u, --sysupgrade    upgrade all packages that are out of date\n"
msgstr "  -u, --sysupgrade    opgrader alle pakker der er forældet\n"

#: ../pacman-g2.c:154
#, c-format
msgid ""
"  -w, --downloadonly  download packages but do not install/upgrade anything\n"
msgstr ""
"  -w, --downloadonly  download pakker men installer/opgrader ikke alting\n"

#: ../pacman-g2.c:155
#, c-format
msgid ""
"  -y, --refresh       download fresh package databases from the server\n"
msgstr "  -y, --refresh       download ny pakke database fra serveren\n"

#: ../pacman-g2.c:156
#, c-format
msgid ""
"      --ignore <pkg>  ignore a package upgrade (can be used more than once)\n"
msgstr ""
"      --ignore <pkg>  ignorere en pakke opgradering (kan bruges mere ind en "
"gang)\n"

#: ../pacman-g2.c:157
#, c-format
msgid ""
"      --nointegrity   don't check the integrity of the packages using sha1\n"
msgstr ""
"      --nointegrity   tjek ikke integriteten af pakkerne ved anvendelse af "
"sha1\n"

#: ../pacman-g2.c:159
#, c-format
msgid "      --config <path> set an alternate configuration file\n"
msgstr "      --config <sti> vælg en alternativ konfigurations fil\n"

#: ../pacman-g2.c:160
#, c-format
msgid "      --noconfirm     do not ask for anything confirmation\n"
msgstr "      --noconfirm     spørg ikke om nogen bekræftelse\n"

#: ../pacman-g2.c:161
#, c-format
msgid "      --ask  <number> pre-specify answers for questions (see manpage)\n"
msgstr ""
"      --ask  <nummer> forrige-specificeret svar på spørgsmål (se man side)\n"

#: ../pacman-g2.c:162
#, c-format
msgid ""
"      --noprogressbar do not show a progress bar when downloading files\n"
msgstr ""
"      --noprogressbar vis ikke et statusmeter når filer bliver hentet\n"

#: ../pacman-g2.c:163
#, c-format
msgid ""
"      --noscriptlet   do not execute the install scriptlet if there is any\n"
msgstr ""
"      --noscriptlet   udfør ikke installations skriptet hvis der er nogen\n"

#: ../pacman-g2.c:164
#, c-format
msgid "  -v, --verbose       be verbose\n"
msgstr "  -v, --verbose       se det hele\n"

#: ../pacman-g2.c:165
#, c-format
msgid "  -r, --root <path>   set an alternate installation root\n"
msgstr "  -r, --root <sti>   vælg en alternativ installations root\n"

#: ../pacman-g2.c:166
#, c-format
msgid "  -b, --dbpath <path> set an alternate database location\n"
msgstr "  -b, --dbpath <sti> vælg en alternativ database placering\n"

