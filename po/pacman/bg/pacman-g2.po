#: ../pacman-g2.c:110
#, c-format
msgid "usage:  %s {-h --help}\n"
msgstr "употреба:  %s {-h --help}\n"

#: ../pacman-g2.c:111
#, c-format
msgid "        %s {-V --version}\n"
msgstr "        %s {-V --version}\n"

#: ../pacman-g2.c:112
#, c-format
msgid "        %s {-A --add}     [options] <file>\n"
msgstr "        %s {-A --add}     [опции] <файл>\n"

#: ../pacman-g2.c:113
#, c-format
msgid "        %s {-R --remove}  [options] <package>\n"
msgstr "        %s {-R --remove}  [опции] <пакет>\n"

#: ../pacman-g2.c:114
#, c-format
msgid "        %s {-U --upgrade} [options] <file>\n"
msgstr "        %s {-U --upgrade} [опции] <файл>\n"

#: ../pacman-g2.c:115
#, c-format
msgid "        %s {-F --freshen} [options] <file>\n"
msgstr "        %s {-F --freshen} [опции] <файл>\n"

#: ../pacman-g2.c:116
#, c-format
msgid "        %s {-Q --query}   [options] [package]\n"
msgstr "        %s {-Q --query}   [опции] [пакет]\n"

#: ../pacman-g2.c:117
#, c-format
msgid "        %s {-S --sync}    [options] [package]\n"
msgstr "        %s {-S --sync}    [опции] [пакет]\n"

#: ../pacman-g2.c:118
#, c-format
msgid ""
"\n"
"use '%s --help' with other options for more syntax\n"
msgstr ""
"\n"
"ползвай '%s --help' с други опции за повече синтаксис\n"

#: ../pacman-g2.c:121
#, c-format
msgid "usage:  %s {-A --add} [options] <file>\n"
msgstr "употреба:  %s {-A --add} [опции] <файл>\n"

#: ../pacman-g2.c:122 ../pacman-g2.c:128 ../pacman-g2.c:140 ../pacman-g2.c:145
#: ../pacman-g2.c:162
#, c-format
msgid "options:\n"
msgstr "опции:\n"

#: ../pacman-g2.c:123 ../pacman-g2.c:130 ../pacman-g2.c:141 ../pacman-g2.c:164
#, c-format
msgid "  -d, --nodeps        skip dependency checks\n"
msgstr "  -d, --nodeps        пропусни проверката за изисквания\n"

#: ../pacman-g2.c:124 ../pacman-g2.c:142 ../pacman-g2.c:166
#, c-format
msgid "  -f, --force         force install, overwrite conflicting files\n"
msgstr ""
"  -f, --force         принудителна инсталация, презапиши конфликтните "
"файлове\n"

#: ../pacman-g2.c:125
#, c-format
msgid ""
"      --noarch        install the package, even if it is for an other arch\n"
msgstr ""
"      --noarch        инсталирай пакета, дори и ако е за друга архитектура\n"

#: ../pacman-g2.c:127
#, c-format
msgid "usage:  %s {-R --remove} [options] <package>\n"
msgstr "употреба:  %s {-R --remove} [опции] <пакет>\n"

#: ../pacman-g2.c:129
#, c-format
msgid ""
"  -c, --cascade       remove packages and all packages that depend on them\n"
msgstr ""
"  -c, --cascade       премахни пакета, и всички пакети, които разчитат на "
"него\n"

#: ../pacman-g2.c:131
#, c-format
msgid "  -k, --dbonly        only remove database entry, do not remove files\n"
msgstr ""
"  -k, --dbonly        премахни само въвеждането в базата данни, не премахвай "
"файлове\n"

#: ../pacman-g2.c:132
#, c-format
msgid "  -n, --nosave        remove configuration files as well\n"
msgstr "  -n, --nosave        премахни и конфигурационните файлове\n"

#: ../pacman-g2.c:133
#, c-format
msgid ""
"  -s, --recursive     remove dependencies also (that won't break packages)\n"
msgstr ""
"  -s, --recursive     премахни и разчитащи пакети (които няма да развалят "
"други пакети)\n"

#: ../pacman-g2.c:136
#, c-format
msgid "usage:  %s {-F --freshen} [options] <file>\n"
msgstr "употреба:  %s {-F --freshen} [опции] <файл>\n"

#: ../pacman-g2.c:138
#, c-format
msgid "usage:  %s {-U --upgrade} [options] <file>\n"
msgstr "употреба:  %s {-U --upgrade} [опции] <файл>\n"

#: ../pacman-g2.c:144
#, c-format
msgid "usage:  %s {-Q --query} [options] [package]\n"
msgstr "употреба:  %s {-Q --query} [опции] [пакет]\n"

#: ../pacman-g2.c:146
#, c-format
msgid "  -c, --changelog     view the changelog of a package\n"
msgstr "  -c, --changelog     вижте списъка с промените на пакет\n"

#: ../pacman-g2.c:147
#, c-format
msgid ""
"  -d, --nodeps        if used with -e, list packages that were not "
"installed\n"
msgstr ""
"  -d, --nodeps       ако се използва с -e, показва пакетите, които не са "
"инсталирани\n"

#: ../pacman-g2.c:148
#, c-format
msgid "                      as a dependency\n"
msgstr "                      като разчитащ пакет\n"

#: ../pacman-g2.c:149
#, c-format
msgid ""
"  -e, --orphans       list all packages that were installed as a dependency\n"
msgstr "  -e, --orphans       покажи пакетите, инсталирани като разчитащи\n"

#: ../pacman-g2.c:150
#, c-format
msgid "                      and are not required by any other packages\n"
msgstr "                      и не се изискват от други пакети\n"

#: ../pacman-g2.c:151 ../pacman-g2.c:167
#, c-format
msgid "  -g, --groups        view all members of a package group\n"
msgstr "  -g, --groups        виж всички членове на пакетна група\n"

#: ../pacman-g2.c:152
#, c-format
msgid "  -i, --info          view package information\n"
msgstr "  -i, --info          виж информацията на пакета\n"

#: ../pacman-g2.c:153
#, c-format
msgid "  -l, --list          list the contents of the queried package\n"
msgstr "  -l, --list          покажи съдържанието на заявения пакет\n"

#: ../pacman-g2.c:154
#, c-format
msgid ""
"  -m, --foreign       list all packages that were not found in the sync db"
"(s)\n"
msgstr ""
"  -m, --foreign       покажи всички пакети, които не са намерени в "
"синхронизираната база данни\n"

#: ../pacman-g2.c:155
#, c-format
msgid "  -o, --owns <file>   query the package that owns <file>\n"
msgstr "  -o, --owns <file>   подай заявка за пакета, който притежава <файл>\n"

#: ../pacman-g2.c:156
#, c-format
msgid ""
"  -p, --file          pacman-g2 will query the package file [package] "
"instead of\n"
msgstr ""
"  -p, --file          pacman-g2  ще подаде заявка за файла на пакета [пакет] "
"вместо за\n"

#: ../pacman-g2.c:157
#, c-format
msgid "                      looking in the database\n"
msgstr "                      търсене в базата данни\n"

#: ../pacman-g2.c:158
#, c-format
msgid ""
"  -s, --search        search locally-installed packages for matching "
"strings\n"
msgstr ""
"  -s, --search        търси местно-инсталирани пакети за съвпадащи string-"
"ове\n"

#: ../pacman-g2.c:159
#, c-format
msgid "  -t, --test          search problems in the local database\n"
msgstr "  -t, --test          търси за проблеми в местната база данни\n"

#: ../pacman-g2.c:161
#, c-format
msgid "usage:  %s {-S --sync} [options] [package]\n"
msgstr "употреба:  %s {-S --sync} [опции] [пакет]\n"

#: ../pacman-g2.c:163
#, c-format
msgid ""
"  -c, --clean         remove old packages from cache directory (use -cc for "
"all)\n"
msgstr ""
"  -c, --clean         премахни стари пакети от кешираната директория "
"(използвай -cc за всички)\n"

#: ../pacman-g2.c:165
#, c-format
msgid "  -e, --dependsonly   install dependencies only\n"
msgstr "  -e, --dependsonly   инсталирай само разчитащите пакети\n"

#: ../pacman-g2.c:168
#, c-format
msgid ""
"  -p, --print-uris    print out URIs for given packages and their "
"dependencies\n"
msgstr ""
"  -p, --print-uris    покажи URI-та за определени пакети и техните разчитащи "
"пакети\n"

#: ../pacman-g2.c:169
#, c-format
msgid "  -s, --search        search remote repositories for matching strings\n"
msgstr ""
"  -s, --search        търси във външни repository-та за съвпадащи string-"
"ове\n"

#: ../pacman-g2.c:170
#, c-format
msgid "  -u, --sysupgrade    upgrade all packages that are out of date\n"
msgstr "  -u, --sysupgrade    обновява всички остарели пакети\n"

#: ../pacman-g2.c:171
#, c-format
msgid ""
"  -w, --downloadonly  download packages but do not install/upgrade anything\n"
msgstr ""
"  -w, --downloadonly  свали пакетите, но не инсталирай и обновявай нищо\n"

#: ../pacman-g2.c:172
#, c-format
msgid ""
"  -y, --refresh       download fresh package databases from the server\n"
msgstr "  -y, --refresh       свали прясна база данни на пакетите от сървъра\n"

#: ../pacman-g2.c:173
#, c-format
msgid ""
"      --ignore <pkg>  ignore a package upgrade (can be used more than once)\n"
msgstr ""
"      --ignore <пакет>  игнорира обновяване на пакет (може да се използва "
"повече от веднъж)\n"

#: ../pacman-g2.c:174
#, c-format
msgid ""
"      --nointegrity   don't check the integrity of the packages using sha1\n"
msgstr "      --nointegrity   не проверявай пакета със sha1\n"

#: ../pacman-g2.c:175
#, c-format
msgid "      --regex         treat targets as regexs if no package found\n"
msgstr ""
"      --regex         третира целите като regexs ако няма намерен пакет\n"

#: ../pacman-g2.c:177
#, c-format
msgid "      --config <path> set an alternate configuration file\n"
msgstr "      --config <път> изберете друг конфигурационен файл\n"

#: ../pacman-g2.c:178
#, c-format
msgid "      --noconfirm     do not ask for anything confirmation\n"
msgstr "      --noconfirm     не питай за потвърждения\n"

#: ../pacman-g2.c:179
#, c-format
msgid "      --ask  <number> pre-specify answers for questions (see manpage)\n"
msgstr ""
"      --ask  <число> предварително задай отговори на въпроси (виж manpage)\n"

#: ../pacman-g2.c:180
#, c-format
msgid ""
"      --noprogressbar do not show a progress bar when downloading files\n"
msgstr ""
"      --noprogressbar не показвай метър на прогреса като се свалят файлове\n"

#: ../pacman-g2.c:181
#, c-format
msgid ""
"      --noscriptlet   do not execute the install scriptlet if there is any\n"
msgstr ""
"      --noscriptlet   не стартирай инсталационния скриптлет, ако има такъв\n"

#: ../pacman-g2.c:182
#, c-format
msgid "  -v, --verbose       be verbose\n"
msgstr "  -v, --verbose       показвай повече информация\n"

#: ../pacman-g2.c:183
#, c-format
msgid "  -r, --root <path>   set an alternate installation root\n"
msgstr "  -r, --root <път>   задайте алтернативен инсталационен root\n"

#: ../pacman-g2.c:184
#, c-format
msgid "  -b, --dbpath <path> set an alternate database location\n"
msgstr "  -b, --dbpath <път> задайте алтернативна база данни\n"

#: ../pacman-g2.c:197
#, c-format
msgid "                       This program may be freely redistributed under\n"
msgstr ""
"                       Тази програма може да бъде преразпространявана "
"свободно под\n"

#: ../pacman-g2.c:198
#, c-format
msgid "                       the terms of the GNU General Public License\n"
msgstr "                       условията на GNU General Public License\n"

#: ../pacman-g2.c:386
msgid "bad root path"
msgstr "лош root път"

#: ../pacman-g2.c:414
msgid "only one operation may be used at a time\n"
msgstr "само по една операция може да бъде използвана"

#: ../pacman-g2.c:514
msgid "you cannot perform this operation unless you are root.\n"
msgstr "не можете да изпълните тази операция, освен ако не сте root.\n"

#: ../pacman-g2.c:538
#, c-format
msgid "failed to initialize pacman library (%s)\n"
msgstr "неуспешно зареждане на библиотеката на pacman (%s)\n"

