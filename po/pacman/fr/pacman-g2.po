msgstr "        %s {-A --add}     [options] <fichier>\n"

#: ../pacman-g2.c:98
#, c-format
msgid "        %s {-R --remove}  [options] <package>\n"
msgstr "        %s {-R --remove}  [options] <paquet>\n"

#: ../pacman-g2.c:99
#, c-format
msgid "        %s {-U --upgrade} [options] <file>\n"
msgstr "        %s {-U --upgrade} [options] <fichier>\n"

