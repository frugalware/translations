msgstr "        %s {-F --freshen} [options] <fichier>\n"

#: ../pacman-g2.c:118
#, c-format
msgid "        %s {-Q --query}   [options] [package]\n"
msgstr "        %s {-Q --query}   [options] [paquet]\n"

#: ../pacman-g2.c:119
#, c-format
msgid "        %s {-S --sync}    [options] [package]\n"
msgstr "        %s {-S --sync}    [options] [paquet]\n"

#: ../pacman-g2.c:120
#, c-format
msgid "        %s {-P --ps}      [options]\n"
msgstr "        %s {-P --ps}      [options]\n"

