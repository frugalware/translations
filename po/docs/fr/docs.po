#: packages.txt:2951
msgid "Create a big empty file:"
msgstr "Créez un gros fichier vide:"

# type: Plain text
#: packages.txt:2954
#, no-wrap
msgid "# dd if=/dev/zero of=root_fs bs=1M count=1000\n"
msgstr "# dd if=/dev/zero of=root_fs bs=1M count=1000\n"

# type: Plain text
#: packages.txt:2957
msgid "Format it:"
msgstr "Formatez le:"

# type: Bullet: '3. '
#: packages.txt:2960
#, no-wrap
msgid "# mke2fs -F -j root_fs\n"
msgstr "# mke2fs -F -j root_fs\n"

