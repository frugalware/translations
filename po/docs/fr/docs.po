msgid ""
"# pacman-g2 -S freenx\n"
"---------------------\n"
msgstr ""
"# pacman-g2 -S freenx\n"
"---------------------\n"

# type: Plain text
#: user.txt:2549 packages.txt:1075
#, no-wrap
msgid ""
"+\n"
"On the client:\n"
"+\n"
"------------------\n"
msgstr ""
"+\n"
"Sur le client:\n"
"+\n"
"------------------\n"

# type: Plain text
#: user.txt:2551 packages.txt:1077
#, no-wrap
msgid ""
"# pacman-g2 -S knx\n"
"------------------\n"
msgstr ""
"# pacman-g2 -S knx\n"
"------------------\n"

# type: Plain text
#: user.txt:2557 packages.txt:1083
#, no-wrap
msgid ""
"2. Configuration\n"
"+\n"
"On the server setup a fake password for the nx user:\n"
"+\n"
"-----------\n"
