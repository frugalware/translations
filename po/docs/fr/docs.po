# type: Plain text
#: packages.txt:167
msgid "Enter \"foobar\" twice as passphrase."
msgstr "Saisir \"foobar\" deux fois en tant que passphrase."

# type: Plain text
#: packages.txt:168 packages.txt:2792 packages.txt:3424 packages.txt:3444
#, no-wrap
msgid "--------------------------------------------------\n"
msgstr "--------------------------------------------------\n"

# type: Plain text
#: packages.txt:170
#, no-wrap
msgid ""
"# openssl req -new -key server.key -out server.csr\n"
"--------------------------------------------------\n"
msgstr ""
"# openssl req -new -key server.key -out server.csr\n"
"--------------------------------------------------\n"

# type: Plain text
#: packages.txt:174
msgid ""
