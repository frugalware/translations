#: user.txt:813 sound.txt:27
#, no-wrap
msgid ""
"# service alsa save\n"
"-------------------\n"
msgstr ""
"# service alsa save\n"
"-------------------\n"

# type: Plain text
#: user.txt:816 sound.txt:30
msgid "and the"
msgstr "et"

# type: Plain text
#: user.txt:819 sound.txt:33
#, no-wrap
msgid ""
"# service alsa load\n"
"-------------------\n"
msgstr ""
"# service alsa load\n"
"-------------------\n"

