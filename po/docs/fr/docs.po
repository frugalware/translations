# type: Plain text
#: user.txt:1284
msgid "Configuration Options: ^^^^^^^^^^^^^^^^^^^^^^"
msgstr "Options de configuration: ^^^^^^^^^^^^^^^^^^^^^^"

# type: Plain text
#: user.txt:1285 user.txt:1605 user.txt:2968 packages.txt:78 packages.txt:398 makepkg.txt:124
#, no-wrap
msgid "-------------------------------------------------------------------------------\n"
msgstr ""

# type: Plain text
#: user.txt:1289 packages.txt:82
#, no-wrap
msgid ""
"eaccelerator.shm_size\n"
"    The amount of shared memory (in megabytes) that eAccelerator will use.\n"
"    \"0\" means OS default. Default value is \"0\".\n"
msgstr ""

# type: Plain text
#: user.txt:1295 packages.txt:88
#, no-wrap
msgid ""
"eaccelerator.cache_dir\n"
"    The directory that is used for disk cache. eAccelerator stores "
"precompiled\n"
"    code, session data, content and user entries  here. The same data  can  "
"be\n"
"    stored in shared memory also (for more quick access). Default value is\n"
"    \"/tmp/eaccelerator\".\n"
msgstr ""

# type: Plain text
#: user.txt:1299 packages.txt:92
#, no-wrap
msgid ""
"eaccelerator.enable\n"
"    Enables or disables eAccelerator. Should be \"1\" for enabling  or  "
"\"0\"  for\n"
"    disabling. Default value is \"1\".\n"
msgstr ""

# type: Plain text
#: user.txt:1304 packages.txt:97
#, no-wrap
msgid ""
"eaccelerator.optimizer\n"
"    Enables or disables internal peephole optimizer which may  speed  up  "
"code\n"
"    execution. Should be \"1\" for enabling or \"0\" for disabling. Default  "
"value\n"
"    is \"1\".\n"
msgstr ""

# type: Plain text
#: user.txt:1308 packages.txt:101
#, no-wrap
msgid ""
"eaccelerator.debug\n"
"    Enables or disables debug logging. Should be \"1\" for enabling or  "
"\"0\"  for\n"
"    disabling. Default value is \"0\".\n"
msgstr ""

# type: Plain text
#: user.txt:1313 packages.txt:106
#, no-wrap
msgid ""
"eaccelerator.check_mtime\n"
"    Enables or disables PHP file modification checking .  Should  be  \"1\"  "
"for\n"
"    enabling or \"0\" for disabling. You should set it to \"1\"  if  you  "
"want  to\n"
"    recompile PHP files after modification. Default value is \"1\".\n"
msgstr ""

# type: Plain text
#: user.txt:1320 packages.txt:113
#, no-wrap
msgid ""
"eaccelerator.filter\n"
"    Determine which PHP files must be cached. You may specify  the  number  "
"of\n"
"    patterns (for example \"*.php *.phtml\") which specifies to cache or not  "
"to\n"
"    cache. If pattern starts with the character \"!\", it means to ignore  "
"files\n"
"    which are matched by the following pattern. Default value is \"\" that "
"means \n"
"    all PHP scripts will be cached.\n"
msgstr ""

# type: Plain text
#: user.txt:1325 packages.txt:118
#, no-wrap
msgid ""
"eaccelerator.shm_max\n"
"    Disables putting large values into shared memory by \" "
"eaccelerator_put() \"\n"
"    function. It indicates the largest allowed size in bytes (10240, 10K, "
"1M). \n"
"    The \"0\" disables the limit. Default value is \"0\".\n"
msgstr ""

# type: Plain text
#: user.txt:1331 packages.txt:124
#, no-wrap
msgid ""
"eaccelerator.shm_ttl\n"
"    When eaccelerator fails to get shared memory for new script it removes "
"all\n"
"    scripts which were not accessed  at  last \"shm_ttl\"  seconds  from  "
"shared\n"
"    memory. Default value is \"0\" that means -  don't  remove  any  files  "
"from\n"
"    shared memory.\n"
msgstr ""

# type: Plain text
#: user.txt:1337 packages.txt:130
#, no-wrap
msgid ""
"eaccelerator.shm_prune_period\n"
"    When eaccelerator fails to get shared memory for new script  it  tryes  "
"to\n"
"    remove  old  script   if   the   previous   try   was   made   more   "
"then\n"
"    \"shm_prune_period\" seconds ago. Default value is \"0\" that  means  -  "
"don't\n"
"    try to remove any files from shared memory.\n"
msgstr ""

# type: Plain text
#: user.txt:1342 packages.txt:135
#, no-wrap
msgid ""
"eaccelerator.shm_only\n"
"    Enables or disables caching of compiled scripts on disk. It has  no  "
"effect\n"
"    on session data and content caching. Default value is \"0\" that means -  "
"use\n"
"    disk and shared memory for caching.\n"
msgstr ""

# type: Plain text
#: user.txt:1346 packages.txt:139
#, no-wrap
msgid ""
"eaccelerator.compress\n"
"    Enables or disables cached content compression. Default value is  \"1\"  "
"that\n"
"    means enable compression.\n"
msgstr ""

# type: Plain text
#: user.txt:1350 packages.txt:143
#, no-wrap
msgid ""
"eaccelerator.compress_level\n"
"    Compression level used for content caching.  Default value is \"9\" "
"which  is\n"
"    the maximum value\n"
msgstr ""

# type: Plain text
#: user.txt:1362 packages.txt:155
#, no-wrap
msgid ""
"eaccelerator.keys\n"
"eaccelerator.sessions\n"
"eaccelerator.content\n"
"    Determine where keys, session data and content will be cached. The "
"possible\n"
"    values are:\n"
"    \"shm_and_disk\" - cache data in shared memory and on disk (default "
"value)\n"
"    \"shm\"          - cache data in shared memory or on disk if shared "
"memory\n"
"                     is full or data size greater then "
"\"eaccelerator.shm_max\"\n"
"    \"shm_only\"     - cache data in shared memory\n"
"    \"disk_only\"    - cache data on disk\n"
"    \"none\"         - don't cache data\n"
msgstr ""

# type: Plain text
#: user.txt:1365 packages.txt:158
msgid "eAccelerator API:"
msgstr ""

# type: Plain text
#: user.txt:1368 packages.txt:161
#, no-wrap
msgid ""
"eaccelerator_put($key, $value, $ttl=0)\n"
"  puts the $value into shard memory for $ttl seconds.\n"
msgstr ""

# type: Plain text
#: user.txt:1372 packages.txt:165
#, no-wrap
msgid ""
"eaccelerator_get($key)\n"
"  returns the value from shared memory which was stored by  "
"eaccelerator_put()\n"
"  or null if it is not exists or was expired.\n"
msgstr ""

# type: Plain text
#: user.txt:1375 packages.txt:168
#, no-wrap
msgid ""
"eaccelerator_rm($key)\n"
"  removres the $key from shared memory\n"
msgstr ""

# type: Plain text
#: user.txt:1378 packages.txt:171
#, no-wrap
msgid ""
"eaccelerator_gc()\n"
"  removes all expired keys from shared memory\n"
msgstr ""

# type: Plain text
#: user.txt:1387 packages.txt:180
#, no-wrap
msgid ""
"eaccelerator_lock($lock)\n"
"  creates a lock with specified name. The lock can  be  released  by  "
"function\n"
"  eaccelerator_unlock() or automatic on the end of request.\n"
"  For Example:\n"
"  <?php\n"
"    eaccelerator_lock(\"count\");\n"
"    eaccelerator_put(\"count\",eaccelerator_get(\"count\")+1));\n"
"  ?>\n"
msgstr ""

# type: Plain text
#: user.txt:1390 packages.txt:183
#, no-wrap
msgid ""
"eaccelerator_unlock($lock)\n"
"  release lock with specified name\n"
msgstr ""

# type: Plain text
#: user.txt:1395 packages.txt:188
#, no-wrap
msgid ""
"eaccelerator_set_session_handlers()\n"
"  install the eaccelerator session handlers.\n"
"  Since PHP 4.2.0 you can install eaccelerator session handlers\n"
"  in \"php.ini\" by \"session.save_handler=eaccelerator\".\n"
msgstr ""

# type: Plain text
#: user.txt:1401 packages.txt:194
#, no-wrap
msgid ""
"eaccelerator_cache_output($key, $eval_code, $ttl=0)\n"
"  caches the output of $eval_code in shared memory for $ttl seconds.\n"
"  Output can be removed from cache by calling mmcach_rm() with the same "
"$key.\n"
"  For Example:\n"
"  <?php eaccelerator_cache_output('test', 'echo time(); phpinfo();', 30); "
"?>\n"
msgstr ""

# type: Plain text
#: user.txt:1407 packages.txt:200
#, no-wrap
msgid ""
"eaccelerator_cache_result($key, $eval_code, $ttl=0)\n"
"  caches the result of $eval_code in shared memory for $ttl seconds.\n"
"  Result can be removed from cache by calling mmcach_rm() with the same "
"$key.\n"
"  For Example:\n"
"  <?php eaccelerator_cache_output('test', 'time().\" Hello\";', 30); ?>\n"
msgstr ""

# type: Plain text
#: user.txt:1416 packages.txt:209
#, no-wrap
msgid ""
"eaccelerator_cache_page($key, $ttl=0)\n"
"  caches the full page for $ttl seconds.\n"
"  For Example:\n"
"  <?php\n"
"    "
"eaccelerator_cache_page($_SERVER['PHP_SELF'].'?GET='.serialize($_GET),30);\n"
"    echo time();\n"
"    phpinfo();\n"
"  ?>\n"
msgstr ""

# type: Plain text
#: user.txt:1420 packages.txt:213
#, no-wrap
msgid ""
"eaccelerator_rm_page($key)\n"
"  removes the page which was cached by eaccelerator_cache_page() with the "
"same\n"
"  $key from cache\n"
msgstr ""

# type: Plain text
#: user.txt:1423 packages.txt:216
#, no-wrap
msgid ""
"eaccelerator_encode($filename)\n"
"  returns the encoded bytecode of compiled file $filename\n"
msgstr ""

# type: Plain text
#: user.txt:1426 packages.txt:219
#, no-wrap
msgid ""
"eaccelerator_load($code)\n"
"  loads script which was encoded by eaccelerator_encode()\n"
"-------------------------------------------------------------------------------\n"
msgstr ""

