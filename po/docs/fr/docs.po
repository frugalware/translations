msgstr "Saisir \"foobar\" deux fois en tant que passphrase."

# type: Plain text
#: user.txt:1612 user.txt:3587 packages.txt:150 packages.txt:2125
#, no-wrap
msgid "--------------------------------------------------\n"
msgstr "--------------------------------------------------\n"

# type: Plain text
#: user.txt:1614 packages.txt:152
#, no-wrap
msgid ""
"# openssl req -new -key server.key -out server.csr\n"
"--------------------------------------------------\n"
msgstr ""
"# openssl req -new -key server.key -out server.csr\n"
"--------------------------------------------------\n"

# type: Plain text
#: user.txt:1618 packages.txt:156
msgid ""
"Enter \"foobar\" when asked for passphrase, answer the questions. Leave "
"\"challenge password\" \"and optional company name\" empty."
msgstr ""
"Saisir \"foobar\" quand on vous demande une passphrase, et répondre aux "
"questions. Laissez vides les champs \"challenge password\" et \"optional "
"company name\"."

# type: Plain text
#: user.txt:1619 user.txt:3509 packages.txt:157 packages.txt:2047
#, no-wrap
msgid "------------------------------------------------\n"
msgstr "------------------------------------------------\n"

# type: Plain text
#: user.txt:1622 packages.txt:160
#, no-wrap
msgid ""
"# cp server.key server.key.org\n"
"# openssl rsa -in server.key.org -out server.key\n"
"------------------------------------------------\n"
msgstr ""
"# cp server.key server.key.org\n"
"# openssl rsa -in server.key.org -out server.key\n"
"------------------------------------------------\n"

# type: Plain text
#: user.txt:1625 packages.txt:163
msgid "Enter \"foobar\" when asked for passphrase."
msgstr "Saisir \"foobar\" quand on vous demande une passphrase."

# type: Plain text
#: user.txt:1632 packages.txt:170
#, no-wrap
msgid ""
"# openssl x509 -req -days 365 -in server.csr -signkey server.key -out server.crt\n"
"# cp server.crt /etc/httpd/conf/ssl.crt/\n"
"# cp server.key /etc/httpd/conf/ssl.key/\n"
"# service httpd stop\n"
