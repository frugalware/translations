#: user.txt:2711 packages.txt:1504
#, no-wrap
msgid "         The name of our city.\n"
msgstr "         Nous entrons le nom de notre ville.\n"

# type: Bullet: '       4) '
#: user.txt:2713 packages.txt:1506
msgid "Organization Name (eg, company) [Snake Oil, Ltd]:"
msgstr "Nom de votre organisation (ex, société) [Snake Oil, Ltd]:"

# type: Plain text
#: user.txt:2715 packages.txt:1508
#, no-wrap
msgid "          Our organization's name.\n"
msgstr "          Nous entrons le nom de notre organisation.\n"

