# type: Plain text
#: packages.txt:2968
#, no-wrap
msgid ""
"# mkdir uml\n"
"# mount root_fs -o loop uml\n"
"# cd uml\n"
msgstr ""
"# mkdir uml\n"
"# mount root_fs -o loop uml\n"
"# cd uml\n"

