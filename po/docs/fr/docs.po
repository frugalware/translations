# type: Plain text
#: user.txt:612 pacman-g2.txt:87
#, no-wrap
msgid ""
"$ pacman-g2 -h\n"
"--------------\n"
msgstr ""
"$ pacman-g2 -h\n"
"--------------\n"

# type: Plain text
#: user.txt:617 pacman-g2.txt:92
msgid ""
