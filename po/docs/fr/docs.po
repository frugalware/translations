msgid ""
"+\n"
"then set a separate (for nx) password for the user:\n"
"+\n"
"------------------------------\n"
msgstr ""
"+\n"
"puis définissez un mot de passe différent (pour nx) pour l'utilisateur:\n"
"+\n"
"------------------------------\n"

# type: Plain text
#: user.txt:2591 packages.txt:1117
#, no-wrap
msgid ""
"# nxserver --passwd <username>\n"
"------------------------------\n"
msgstr ""
"# nxserver --passwd <username>\n"
"------------------------------\n"

# type: Plain text
#: user.txt:2596 packages.txt:1122
#, no-wrap
