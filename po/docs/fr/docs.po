msgstr "-------------------------------\n"

# type: Plain text
#: packages.txt:310
#, no-wrap
msgid ""
"# iwconfig ethX essid \"myessid\"\n"
"-------------------------------\n"
msgstr ""
"# iwconfig ethX essid \"myessid\"\n"
"-------------------------------\n"

