msgstr ""

# type: Plain text
#. type: Plain text
#: packages.txt:73
msgid "command."
msgstr ""

# type: Plain text
#. type: Plain text
#: packages.txt:75
msgid "We don't want to reboot, so start it manually:"
msgstr ""

