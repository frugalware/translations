"-A INPUT -p tcp -m tcp --dport 8080 -j ACCEPT\n"
msgstr ""
"# httpircproxy\n"
"-A INPUT -p tcp -m tcp --dport 8080 -j ACCEPT\n"

# type: Plain text
#: packages.txt:1478 packages.txt:1923
