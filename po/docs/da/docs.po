msgid "== rss2email"
msgstr ""

# type: Plain text
#: packages.txt:2571
msgid "Configure: ~~~~~~~~~~"
msgstr ""

# type: Plain text
#: packages.txt:2573
msgid "Create a new feed database:"
msgstr ""

# type: Plain text
#: packages.txt:2576
#, no-wrap
msgid ""
"$ r2e new you@yourdomain.com\n"
"----------------------------\n"
msgstr ""

# type: Plain text
#: packages.txt:2579
msgid "Subscribe to some feeds:"
msgstr ""

# type: Plain text
#: packages.txt:2582
#, no-wrap
msgid ""
