# type: Plain text
#: packages.txt:2844
msgid "== user-mode-linux"
msgstr ""

# type: Plain text
#: packages.txt:2846
msgid "=== Creating a root image"
msgstr ""

# type: Plain text
#: packages.txt:2848
msgid "Create a big empty file:"
msgstr ""

# type: Plain text
#: packages.txt:2851
#, no-wrap
msgid ""
"# dd if=/dev/zero of=root_fs bs=1M count=1000\n"
"----\n"
msgstr ""

# type: Plain text
#: packages.txt:2854
msgid "Format it:"
msgstr ""

# type: Plain text
#: packages.txt:2857
#, no-wrap
msgid ""
"# mke2fs -F -j root_fs\n"
"----\n"
msgstr ""

# type: Plain text
#: packages.txt:2860
msgid "Mount it:"
msgstr ""

# type: Plain text
#: packages.txt:2865
#, no-wrap
msgid ""
"# mkdir uml\n"
"# mount root_fs -o loop uml\n"
"# cd uml\n"
"----\n"
msgstr ""

# type: Plain text
#: packages.txt:2868
msgid "Install `base` and `openssh`:"
msgstr ""

# type: Plain text
#: packages.txt:2872
#, no-wrap
msgid ""
"# mkdir -p var/log tmp\n"
"# pacman-g2 -Sy base openssh -r ./\n"
"----\n"
msgstr ""

# type: Plain text
#: packages.txt:2875
msgid "Create `etc/fstab` with the following contents:"
msgstr ""

# type: Plain text
#: packages.txt:2881
#, no-wrap
msgid ""
"none             /proc            proc        defaults         0   0\n"
"none             /sys             sysfs       defaults         0   0\n"
"devpts           /dev/pts         devpts      gid=5,mode=620   0   0\n"
"/dev/ubda        /                ext3        defaults         1   1\n"
"----\n"
msgstr ""

# type: Plain text
#: packages.txt:2884
msgid "Create `etc/sysconfig/keymap` with the following contents:"
msgstr ""

# type: Plain text
#: packages.txt:2887
#, no-wrap
msgid ""
"keymap=us\n"
"----\n"
msgstr ""

# type: Plain text
#: packages.txt:2890
msgid "Create `etc/profile.d/lang.sh` with the following contents:"
msgstr ""

# type: Plain text
#: packages.txt:2894
#, no-wrap
msgid ""
"export LANG=en_US\n"
"export LC_ALL=$LANG\n"
"----\n"
msgstr ""

# type: Plain text
#: packages.txt:2897
msgid ""
"We want networking, put the followings to `etc/sysconfig/network/default`:"
msgstr ""

# type: Plain text
#: packages.txt:2902
#, no-wrap
msgid ""
"[eth0]\n"
"options = 192.168.0.1\n"
"gateway = default gw 192.168.0.254\n"
"----\n"
msgstr ""

# type: Plain text
#: packages.txt:2906
msgid ""
"If you want to use multiple virtual machines, use `192.168.0.2`, "
"`192.168.0.3` and so on instead."
msgstr ""

# type: Plain text
#: packages.txt:2908
msgid "Let's copy in the terminal device and change our root:"
msgstr ""

# type: Plain text
#: packages.txt:2912
#, no-wrap
msgid ""
"# cp -a /dev/tty dev/\n"
"# chroot ./\n"
"----\n"
msgstr ""

# type: Plain text
#: packages.txt:2915
msgid "Create a regular user:"
msgstr ""

# type: Plain text
#: packages.txt:2918
#, no-wrap
msgid ""
"# adduser\n"
"----\n"
msgstr ""

# type: Plain text
#: packages.txt:2921
msgid "Remove unnecessary services and enable ssh:"
msgstr ""

# type: Plain text
#: packages.txt:2927
#, no-wrap
msgid ""
