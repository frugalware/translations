# type: Plain text
#: packages.txt:1025
#, no-wrap
msgid ""
"# openssl rsa -in privkey.pem -out privkey.pem\n"
"----\n"
msgstr ""

# type: Plain text
#: packages.txt:1028
msgid "Combine the Private and Public Key:"
msgstr ""

# type: Plain text
#: packages.txt:1031
#, no-wrap
msgid ""
"# cat privkey.pem >> server.pem\n"
"----\n"
msgstr ""

# type: Plain text
#: packages.txt:1034
msgid "Delete Private Key:"
msgstr ""

# type: Plain text
#: packages.txt:1037
#, no-wrap
msgid ""
"# rm privkey.pem\n"
"----\n"
msgstr ""

# type: Plain text
#: packages.txt:1040
msgid "Set permissions:"
msgstr ""

# type: Plain text
#: packages.txt:1044
#, no-wrap
msgid ""
"# chown root:ejabberd server.pem    \n"
"# chmod 640 server.pem\n"
"----\n"
msgstr ""

# type: Plain text
#: packages.txt:1047
msgid "Finally update the config file:"
msgstr ""

# type: Plain text
#: packages.txt:1049
#, no-wrap
