# type: Plain text
#: user.txt:2020 packages.txt:503
#, no-wrap
msgid ""
"smtpd_sasl_auth_enable = yes\n"
"smtpd_sasl_local_domain = $myhostname\n"
"smtpd_sasl_security_options = noanonymous\n"
"--------------\n"
msgstr ""

# type: Plain text
#: user.txt:2023 packages.txt:506
msgid "You may want to append"
msgstr ""

# type: Plain text
#: user.txt:2026 packages.txt:509
#, no-wrap
msgid ""
"broken_sasl_auth_clients = yes\n"
"--------------\n"
msgstr ""

# type: Plain text
#: user.txt:2029 packages.txt:512
msgid "as well."
msgstr ""

# type: Plain text
#: user.txt:2031 packages.txt:514
msgid "Put the following lines to `/usr/lib/sasl2/smtpd.conf`:"
msgstr ""

# type: Plain text
#: user.txt:2035 packages.txt:518
#, no-wrap
msgid ""
"pwcheck_method: saslauthd\n"
"mech_list: PLAIN LOGIN\n"
"--------------\n"
msgstr ""

# type: Plain text
#: user.txt:2038 packages.txt:521
msgid "Edit `/etc/sysconfig/saslauthd` by changing the following lines:"
