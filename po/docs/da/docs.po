msgid "---------------------------------------\n"
msgstr "--------------------------------------------------------------------------\n"

# type: Plain text
#: user.txt:4552 packages.txt:3082
#, no-wrap
msgid ""
" Section \"Extensions\"\n"
"    Option     \"Composite\" \"Enable\"\n"
" EndSection\n"
"---------------------------------------\n"
msgstr ""

# type: Plain text
#: user.txt:4555 packages.txt:3085
msgid ""
