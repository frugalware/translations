#: packages.txt:1012
msgid "Generate Key Pair:"
msgstr ""

# type: Plain text
#: packages.txt:1016
#, no-wrap
msgid ""
"# cd /etc/jabberd\n"
"# openssl req -new -x509 -newkey rsa:1024 -days 3650 -keyout privkey.pem -out server.pem\n"
