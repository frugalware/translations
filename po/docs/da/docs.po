# type: Plain text
#: user.txt:2357
msgid "==== Creating your SSL keys"
msgstr ""

# type: Plain text
#: user.txt:2359 packages.txt:889
msgid "Generate Key Pair:"
msgstr ""

# type: Plain text
#: user.txt:2363 packages.txt:893
#, no-wrap
msgid ""
"# cd /etc/jabberd\n"
"# openssl req -new -x509 -newkey rsa:1024 -days 3650 -keyout privkey.pem -out server.pem\n"
"----\n"
msgstr ""

# type: Plain text
#: user.txt:2367 packages.txt:897
msgid ""
"NOTE: You should enter your domain name as the Common Name for your "
"certificate."
msgstr ""

# type: Plain text
#: user.txt:2369 packages.txt:899
msgid "Remove pass parse:"
msgstr ""

# type: Plain text
#: user.txt:2372 packages.txt:902
#, no-wrap
msgid ""
"# openssl rsa -in privkey.pem -out privkey.pem\n"
"----\n"
msgstr ""

# type: Plain text
#: user.txt:2375 packages.txt:905
msgid "Combine the Private and Public Key:"
msgstr ""

# type: Plain text
#: user.txt:2378 packages.txt:908
#, no-wrap
msgid ""
"# cat privkey.pem >> server.pem\n"
"----\n"
msgstr ""

# type: Plain text
#: user.txt:2381 packages.txt:911
msgid "Delete Private Key:"
msgstr ""

# type: Plain text
#: user.txt:2384 packages.txt:914
#, no-wrap
msgid ""
"# rm privkey.pem\n"
"----\n"
msgstr ""

# type: Plain text
#: user.txt:2387 packages.txt:917
msgid "Set permissions:"
msgstr ""

# type: Plain text
#: user.txt:2391 packages.txt:921
#, no-wrap
msgid ""
"# chown root:ejabberd server.pem    \n"
"# chmod 640 server.pem\n"
"----\n"
msgstr ""

# type: Plain text
#: user.txt:2394 packages.txt:924
msgid "Finally update the config file:"
msgstr ""

# type: Plain text
#: user.txt:2396 packages.txt:926
#, no-wrap
msgid "* Change the `./ssl.pem` string to `/etc/ejabberd/server.pam`.\n"
msgstr ""

# type: Plain text
#: user.txt:2399 packages.txt:929
#, no-wrap
msgid ""
"* Change `starttls` to `tls` in the `listen` section if you want to force users\n"
"  to use SSL.\n"
msgstr ""

