# type: Plain text
#: user.txt:4012 packages.txt:2542
#, no-wrap
msgid ""
"none             /proc            proc        defaults         0   0\n"
"none             /sys             sysfs       defaults         0   0\n"
"devpts           /dev/pts         devpts      gid=5,mode=620   0   0\n"
"/dev/ubda        /                ext3        defaults         1   1\n"
"----\n"
msgstr ""

# type: Plain text
#: user.txt:4015 packages.txt:2545
msgid "Create `etc/sysconfig/keymap` with the following contents:"
msgstr "Hozzuk létre a `etc/sysconfig/keymap` fájlt a következő tartalommal:"

# type: Plain text
#: user.txt:4018 packages.txt:2548
#, no-wrap
msgid ""
"keymap=us\n"
"----\n"
msgstr ""

# type: Plain text
#: user.txt:4021 packages.txt:2551
msgid "Create `etc/profile.d/lang.sh` with the following contents:"
