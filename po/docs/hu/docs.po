# type: Plain text
#: user.txt:4025 packages.txt:2555
#, no-wrap
msgid ""
"export LANG=en_US\n"
"export LC_ALL=$LANG\n"
"----\n"
msgstr ""

# type: Plain text
#: user.txt:4028 packages.txt:2558
msgid ""
"We want networking, put the followings to `etc/sysconfig/network/default`:"
msgstr ""

# type: Plain text
#: user.txt:4033 packages.txt:2563
#, no-wrap
msgid ""
"[eth0]\n"
"options = 192.168.0.1\n"
"gateway = default gw 192.168.0.254\n"
"----\n"
msgstr ""

# type: Plain text
#: user.txt:4037 packages.txt:2567
msgid ""
"If you want to use multiple virtual machines, use `192.168.0.2`, "
"`192.168.0.3` and so on instead."
msgstr ""

# type: Plain text
#: user.txt:4039 packages.txt:2569
msgid "Let's copy in the terminal device and change our root:"
msgstr ""

# type: Plain text
#: user.txt:4043 packages.txt:2573
#, no-wrap
msgid ""
"# cp -a /dev/tty dev/\n"
"# chroot ./\n"
"----\n"
msgstr ""

# type: Plain text
#: user.txt:4046 packages.txt:2576
msgid "Create a regular user:"
