# type: Plain text
#: user.txt:4003 packages.txt:2533
#, no-wrap
msgid ""
"# mkdir -p var/log tmp\n"
"# pacman-g2 -Sy base openssh -r ./\n"
"----\n"
msgstr ""

# type: Plain text
#: user.txt:4006 packages.txt:2536
msgid "Create `etc/fstab` with the following contents:"
