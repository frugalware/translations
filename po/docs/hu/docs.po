msgstr "'AllowRootLogin=true'"

# type: Plain text
#: user.txt:780 x.txt:56
msgid "For GDM ('/etc/gdm/gdm.conf')"
msgstr "GDM esetén ('/etc/gdm/gdm.conf')"

# type: Plain text
#: user.txt:782 x.txt:58
msgid "'AllowRoot=false'"
msgstr "'AllowRoot=false'"

# type: Plain text
