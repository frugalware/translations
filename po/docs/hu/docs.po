#: user.txt:793 sound.txt:27
#, no-wrap
msgid ""
"# service alsa save\n"
"-------------------\n"
msgstr ""
"# service alsa save\n"
"-------------------\n"

# type: Plain text
#: user.txt:796 sound.txt:30
