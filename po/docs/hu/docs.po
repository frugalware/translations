# type: Plain text
#: user.txt:3979 packages.txt:2509
msgid "Create a big empty file:"
msgstr ""

# type: Plain text
#: user.txt:3982 packages.txt:2512
#, no-wrap
msgid ""
"# dd if=/dev/zero of=root_fs bs=1M count=1000\n"
"----\n"
msgstr ""

# type: Plain text
#: user.txt:3985 packages.txt:2515
msgid "Format it:"
