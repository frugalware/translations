# type: Plain text
#: user.txt:1130 init.txt:104
#, no-wrap
msgid ""
"# service $service_name$ del\n"
"----------------------------\n"
msgstr ""
"# service $szolgáltatás_neve$ del\n"
"n----------------------------\n"

# type: Plain text
