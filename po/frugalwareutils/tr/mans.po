#: netconfig.txt:6
msgid "netconfig - manages network configuration profiles"
msgstr "netconfig - ağ yapılandırma profillerinizi yönetir"

# type: Plain text
#: netconfig.txt:10
msgid "netconfig [options] start|stop|restart|status|list [interface]"
msgstr "netconfig [seçenekler] start|stop|restart|status|list [arayüz]"

# type: Plain text
#: netconfig.txt:12
msgid "netconfig [options] [profile] [interface]"
msgstr "netconfig [seçenekler] [profil] [arayüz]"

# type: Plain text
#: netconfig.txt:17
#, no-wrap
msgid ""
"-h, --help::\n"
"\tDisplay this manpage.\n"
msgstr ""
"--help::\n"
"\tBu kılavuz sayfası görüntülenir.\n"

# type: Plain text
#: netconfig.txt:20
#, no-wrap
msgid ""
"-f, --fast::\n"
"\tFast mode, used by the installer.\n"
msgstr ""
"-f, --fast::\n"
"\tKurulumcu tarafından kullanılan hızlı kip.\n"

# type: Plain text
#: netconfig.txt:23
#, no-wrap
msgid ""
"--dry-run::\n"
"\tDo not actually perform the operation.\n"
msgstr ""
"--dry-run::\n"
"\tAslında bu işlemi kullanmayınız.\n"

# type: Plain text
#: netconfig.txt:26
#, no-wrap
msgid ""
"--loup::\n"
"\tBring up the loopback interface.\n"
msgstr ""
"--loup::\n"
"\tGeribildirim arayüzünü etkinleştir.\n"

# type: Plain text
#: netconfig.txt:29
#, no-wrap
