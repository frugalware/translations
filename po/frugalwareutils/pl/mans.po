# type: Plain text
#: mouseconfig.txt:2
msgid "= mouseconfig(1)"
msgstr "= mouseconfig(1)"

# type: Plain text
#: mouseconfig.txt:6
msgid "mouseconfig - configures your mouse settings"
msgstr "mouseconfig - konfiguruje ustawienia myszy"

# type: Plain text
#: mouseconfig.txt:10
