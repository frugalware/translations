#: ../src/gservice.c:53
msgid "Insufficient privileges"
msgstr "Không đủ quyền"

#: ../src/gservice.c:53
msgid "gService must be run as root."
msgstr "gService phải chạy dưới người chủ (root)."

#: ../src/gservice.c:63
msgid ""
"Failed to initialize interface due to the following error(s) in gservice.ui:"
msgstr "Không sơ khởi được giao diện do những lỗi theo đây trong gservice.ui:"

#: ../src/gservice.c:65
msgid "Interface initialization Failed"
msgstr "Lỗi sơ khởi giao diện"

#: ../src/gservice.c:77 ../src/gservice-interface.c:826
