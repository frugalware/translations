#: ../configsave.sh:12 ../fileswap.sh:11 ../fpm2lzm.sh:33
msgid "Only root can run %s.\\n"
msgstr "Chỉ người chủ (root) có quyền chạy %s.\n"

#: ../configsave.sh:23
msgid "usage: %s /media/sda?/%s\\n"
msgstr "sử dụng: %s /media/sda?/%s\n"

#: ../configsave.sh:24
msgid "Not a regular file: %s\\n"
msgstr "Không phải một tập tin thông thường: %s\n"

#: ../configsave.sh:28
msgid "This script can work only from FWLive."
msgstr "Văn lệnh này chỉ có thể hoạt động từ FWLive thôi."

#: ../configsave.sh:40
msgid "Saving changes to %s... "
msgstr "Đang lưu các thay đổi trong %s..."

#: ../configsave.sh:45
msgid "Can't write config to %s\\n"
msgstr "Không thể ghi cấu hình vào %s\n"

#: ../configsave.sh:47 ../configsave.sh:58
msgid "done."
msgstr "hoàn tất."

#: ../configsave.sh:55
msgid "Restoring changes from %s... "
msgstr "Đang phục hồi các thay đổi từ %s..."

#: ../fileswap.sh:18
msgid "File Swap - create swap as a file in writable filesystem"
msgstr ""
"Trao đổi Tập tin — tạo vùng trao đổi dạng một tập tin trên hệ thống tập tin "
"cho phép ghi"

#: ../fileswap.sh:19
msgid "usage: %s [/media/partition/new_name.swap] [size MB]\\n"
msgstr "sử dụng: %s [/media/phân_vùng/tên_mới.swap] [cỡ MB]\n"

#: ../fileswap.sh:29
msgid "you must use path in /media"
msgstr "Cần phải đặt đường dẫn từ « /media »"

#: ../fileswap.sh:33
msgid "creating empty %s\\n"
msgstr "đang tạo trống %s\n"

#: ../fileswap.sh:37
msgid "setting up Linux swap area on %s\\n"
msgstr "đang thiết lập vùng trao đổi Linux trên %s\n"

#: ../fileswap.sh:41
msgid "activating swap on %s\\n"
msgstr "đang kích hoạt vùng trao đổi trên %s\n"

#: ../fpm2lzm.sh:37
msgid "usage: %s out.lzm pkg1 [ pkg2 [ ... ] ]\\n"
msgstr "sử dụng: %s xuất.lzm gói1 [ gói2 [ ... ] ]\n"

#: ../rc.config.sh:30
msgid "Searching for a previous FWLive configuration file"
msgstr "Đang tìm kiếm một tập tin cấu hình FWLive tạo trước"

#: ../rc.config.sh:33 ../rc.config.sh:67
msgid "Found an FWLive config file."
msgstr "Tìm thấy một tập tin cấu hình FWLive."

#: ../rc.config.sh:34
msgid "Do you want to restore saved settings from %s?\\n"
msgstr "Bạn có muốn phục hồi từ %s thiết lập đã lưu không?\n"

#: ../rc.config.sh:39 ../rc.config.sh:73
msgid "No previous FWLive config file found."
msgstr "Không tìm thấy tập tin cấu hình FWLive tạo trước."

#: ../rc.config.sh:48
msgid "enter=confirm"
msgstr "enter=xác_nhận"

#: ../rc.config.sh:56
msgid "\\nTimed out. Never mind, skipped."
msgstr ""
"\n"
"Quá hạn. Không sao, chỉ bỏ qua."

#. automatically restore/save settings in the case fwliveconf.lzm is found
#. in root directory of any partition. Only the first one is used.
#: ../rc.config.sh:64
msgid "Storing FWLive configuration file"
msgstr "Đang cất giữ tập tin cấu hình FWLive"

#: ../rc.config.sh:68
msgid "Save settings to %s?\\n"
msgstr "Lưu thiết lập vào %s ?\n"

#: ../rc.config.sh:74
msgid "Do you want to save settings?"
msgstr "Bạn có muốn lưu thiết lập không?"

#: ../rc.config.sh:75
msgid "Where do you want to save your settings? [example: /media/hda1]: "
msgstr "Bạn có muốn lưu thiết lập vào thư mục nào? [v.d. /media/hda1]:"

#: ../rc.config.sh:81
msgid "No such directory."
msgstr "Không có thư mục như vậy."

#: ../rc.fsupd.sh:16
msgid "fstab updater daemon"
msgstr "Trình nền cập nhật fstab"

#: ../rc.fwlive.sh:16
msgid "\\e[01mWelcome to \\e[01;36mFwlive\\e[0m\\e[01m!\\e[0m"
msgstr "\\e[01mChào mừng đến \\e[01;36mFwlive\\e[0m\\e[01m!\\e[0m"

#: ../rc.parse_cmdline.sh:17
msgid "Parsing /proc/cmdline"
msgstr "Đang phân tích cú pháp của « /proc/cmdline »"

#. eg. Welcome to FWLive 1.0 by Frugalware
#: ../parse_cmdline.sh:49
msgid "Welcome to FWLive %s by %s"
msgstr "Chào mừng đến FWLive %s của %s"

#: ../parse_cmdline.sh:53
msgid "The system is up and running now."
msgstr "Hệ thống đang chạy."

#: ../parse_cmdline.sh:54
msgid "Login as '%s', with password '%s' or"
msgstr "Đăng nhập dưới « %s », với mật khẩu « %s », hoặc"

#: ../parse_cmdline.sh:56
msgid "login as 'root', with password '%s', both without quotes, lowercase."
msgstr "đang nhập dưới « root », với mật khẩu « %s » (cả hai chữ thường)."

#: ../parse_cmdline.sh:57
msgid "After login, try the following commands:"
msgstr "Một khi đăng nhập, thử chạy những câu lệnh theo đây:"

#: ../parse_cmdline.sh:58
msgid ""
"mc .... to start Midnight Commander (edit/copy/move/create/delete files)"
msgstr ""
"mc ... để khởi chạy Midnight Commander (sửa/chép/chuyển/tạo/xoá tập tin)"

#: ../parse_cmdline.sh:59
msgid "configsave/configrestore  - or use - service config save/restore ..."
msgstr ""
"configsave/configrestore  — hoặc dùng — service config save/restore ..."

#: ../parse_cmdline.sh:60
msgid "to save and restore all filesystem changes"
msgstr "để lưu và phục hồi các thay đổi về hệ thống tập tin"

#: ../parse_cmdline.sh:61
msgid "fileswap .... to create special file for swapping RAM to your harddisk"
msgstr ""
"fileswap .... tạo tập tin đặc biệt để trao đổi bộ nhớ RAM với phần cứng"

#: ../parse_cmdline.sh:62
msgid "make_disk .... to install FWLive, use make_disk script on FWLive CD"
msgstr ""
"make_disk .... cài đặt FWLive, dùng văn lệnh make_disk trên đĩa CD FWLive"

#: ../parse_cmdline.sh:63
msgid ""
"xstart .... to start KDE with autodetected settings (upon failure vesa "
"driver will be used)"
msgstr ""
"xstart .... khởi chạy KDE với thiết lập tự động phát hiện (gặp lỗi thì dùng "
"trình điều khiển vesa)"

#: ../parse_cmdline.sh:64
msgid "setup .... a script for your own settings"
msgstr "setup .... văn lệnh cho thiết lập riêng của người dùng"

#: ../parse_cmdline.sh:65
msgid ""
"When finished, use 'poweroff' or 'reboot' command and wait until it completes"
msgstr ""
"Một khi hoàn tất thì sử dụng lệnh tắt điện « poweroff » hay lệnh khởi động "
"lại «  reboot » và đợi đến khi nó chạy xong"
