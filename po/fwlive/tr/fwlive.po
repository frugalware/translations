#: ../fileswap.sh:18
msgid "File Swap - create swap as a file in writable filesystem"
msgstr ""
"File Swap - dosya sisteminde dosya gibi yazılabilir bir takas alanı "
"oluşturur"

#: ../fileswap.sh:19
msgid "usage: %s [/media/partition/new_name.swap] [size MB]\\n"
msgstr "kullanım: %s [/media/partition/yeni_isim.swap] [boyut MB]\n"

#: ../fileswap.sh:29
msgid "you must use path in /media"
msgstr "/media altında bir yol kullanmak zorundasınız"

#: ../fileswap.sh:33
msgid "creating empty %s\\n"
msgstr "boş %s oluşturuluyor\n"

#: ../fileswap.sh:37
msgid "setting up Linux swap area on %s\\n"
msgstr "%s üzerinde Linux takas alanı ayarlanıyor\n"

#: ../fileswap.sh:41
msgid "activating swap on %s\\n"
msgstr "%s üzerindeki takas alanı etkinleştiriliyor"

#: ../fpm2lzm.sh:37
msgid "usage: %s out.lzm pkg1 [ pkg2 [ ... ] ]\\n"
msgstr "kullanım: %s out.lzm pkg1 [ pkg2 [ ... ] ]\n"

