#: ../configsave.sh:12 ../fileswap.sh:11 ../fpm2lzm.sh:33
msgid "Only root can run %s.\\n"
msgstr "Csak rendszergazda futtathatja a következõt: %s.\\n"

#: ../configsave.sh:23
msgid "usage: %s /media/sda?/%s\\n"
msgstr "használat: %s /media/sda?/%s\\n"

#: ../configsave.sh:24
msgid "Not a regular file: %s\\n"
msgstr "Nem megszokott file: %s\\n"

#: ../configsave.sh:28
msgid "This script can work only from FWLive."
msgstr "Ez a szkript csak az FWLive-ból tud futni."

#: ../configsave.sh:40
msgid "Saving changes to %s... "
msgstr "Változásáok elmentése a %s file-ba... "

#: ../configsave.sh:45
msgid "Can't write config to %s\\n"
msgstr "Nem sikerült írni a %s file-ba\\n"

#: ../configsave.sh:47 ../configsave.sh:58
msgid "done."
msgstr "kész."

#: ../configsave.sh:55
msgid "Restoring changes from %s... "
msgstr "Változások visszaállítása a %s file-ból... "

#: ../fileswap.sh:18
msgid "File Swap - create swap as a file in writable filesystem"
msgstr "File Swap - virtuális memóriát hoz létre egy írható filerendszeren"

#: ../fileswap.sh:19
msgid "usage: %s [/media/partition/new_name.swap] [size MB]\\n"
msgstr "használat: %s [/media/paríció/új_név.swap] [méret MB-ban]\\n"

#: ../fileswap.sh:29
msgid "you must use path in /media"
msgstr "az útvonalnak a /media-n belül kell lennie"

#: ../fileswap.sh:33
msgid "creating empty %s\\n"
msgstr "üres %s létrehozása\\n"

#: ../fileswap.sh:37
msgid "setting up Linux swap area on %s\\n"
msgstr "Linux virtuális memória létrehozása itt: %s\\n"

#: ../fileswap.sh:41
msgid "activating swap on %s\\n"
msgstr "virtuális memória engedélyezése: %s\\n"

#: ../fpm2lzm.sh:37
msgid "usage: %s out.lzm pkg1 [ pkg2 [ ... ] ]\\n"
msgstr "használat: %s kimenet.lzm csomag1 [ csomag2 [ ... ] ]\\n"

#: ../rc.config.sh:30
msgid "Searching for a previous FWLive configuration file"
msgstr "Elõzõ FWLive beállítás keresése"

#: ../rc.config.sh:33 ../rc.config.sh:67
msgid "Found an FWLive config file."
msgstr "Találtam egy FWLive beállítási file-t."

#: ../rc.config.sh:34
msgid "Do you want to restore saved settings from %s?\\n"
msgstr "Szeretné visszaállítani az elmentett beállításokat innen: %s?\\n"

#: ../rc.config.sh:39 ../rc.config.sh:73
msgid "No previous FWLive config file found."
msgstr "Nem találtam elõzõ FWLive beállítási file-t."

#: ../rc.config.sh:48
msgid "enter=confirm"
msgstr "enter=megerõsítés"

#: ../rc.config.sh:56
msgid "\\nTimed out. Never mind, skipped."
msgstr "\\nIdõtúllépés. Mindegy, kihagyom."

#. automatically restore/save settings in the case fwliveconf.lzm is found
#. in root directory of any partition. Only the first one is used.
#: ../rc.config.sh:64
msgid "Storing FWLive configuration file"
msgstr "Az FWLive beállítások elmentése"

#: ../rc.config.sh:68
msgid "Save settings to %s?\\n"
msgstr "Elmenti a beállításokat ide: %s?\\n"

#: ../rc.config.sh:74
msgid "Do you want to save settings?"
msgstr "Szeretné elmenteni a beállításokat?"

#: ../rc.config.sh:75
msgid "Where do you want to save your settings? [example: /media/hda1]: "
msgstr "Hova szeretné elmenteni a beállításokat? [példa: /media/hda1]:"

#: ../rc.config.sh:81
msgid "No such directory."
msgstr "Nincs ilyen könyvtár."

#: ../rc.fsupd.sh:16
msgid "fstab updater daemon"
msgstr "fstab frissítõ daemon"

#: ../rc.fwlive.sh:16
msgid "\\e[01mWelcome to \\e[01;36mFwlive\\e[0m\\e[01m!\\e[0m"
msgstr "\\e[01mÜdvözli az \\e[01;36mFwlive\\e[0m\\e[01m!\\e[0m"

#: ../rc.parse_cmdline.sh:17
msgid "Parsing /proc/cmdline"
msgstr "Boot paraméterek feldolgozása"

#: ../parse_cmdline.sh:48
msgid "Welcome to FWLive %s by %s"
msgstr "Üdvözli az FWLive %s a %s fejlesztõitõl"

#: ../parse_cmdline.sh:52
msgid "The system is up and running now."
msgstr "A rendszer betöltése befejezõdött."

#: ../parse_cmdline.sh:53
msgid "Login as '%s', with password '%s' or"
msgstr ""
"Bejelentkezéshez használja a(z) '%s' felhasználónevet a(z) '%s' jelszóval, "
"vagy"

#: ../parse_cmdline.sh:55
msgid "login as 'root', with password '%s', both without quotes, lowercase."
msgstr ""
"lépjen be 'root'-ként a(z) '%s' jelszóval, mindkettõ idézõjelek nélkül, "
"kisbetûvel."

#: ../parse_cmdline.sh:56
msgid "After login, try the following commands:"
msgstr "Bejelentkezés után próbálja ki a következõ parancsokat:"

#: ../parse_cmdline.sh:57
msgid ""
"mc .... to start Midnight Commander (edit/copy/move/create/delete files)"
msgstr ""
"mc .... Midnight Commander indítása (fájlok szerkesztése/másolása/mozgatása/"
"létrehozása/törlése)"

#: ../parse_cmdline.sh:58
msgid "configsave/configrestore  - or use - service config save/restore ..."
msgstr ""
"configsave/configrestore  - vagy használható a - service config save/"
"restore ..."

#: ../parse_cmdline.sh:59
msgid "to save and restore all filesystem changes"
msgstr "a rendszerben történt változások mentésére és visszaállítására"

#: ../parse_cmdline.sh:60
msgid "fileswap .... to create special file for swapping RAM to your harddisk"
msgstr ""
"fileswap .... készíthet egy speciális fájlt a merevlemezre, virtuális "
"memória céljából"

#: ../parse_cmdline.sh:61
msgid "make_disk .... to install FWLive, use make_disk script on FWLive CD"
msgstr "make_disk .... az FWLive telepítéséhez ezt a scriptet használja"

#: ../parse_cmdline.sh:62
msgid ""
"xstart .... to start KDE with autodetected settings (upon failure vesa "
"driver will be used)"
msgstr ""
"xstart .... KDE indítása automatikusan érzékelt beállításokkal (hiba esetén "
"a VESA meghajtót használunk)"

#: ../parse_cmdline.sh:63
msgid "setup .... a script for your own settings"
msgstr "setup .... beállító program a saját beállításainak"

#: ../parse_cmdline.sh:64
msgid ""
"When finished, use 'poweroff' or 'reboot' command and wait until it completes"
msgstr ""
"Ha végzett, használja a 'poweroff' vagy a 'reboot' parancsot és várja meg "
"amíg a rendszer leáll"

