#: ../configsave.sh:12 ../fileswap.sh:11 ../fpm2lzm.sh:33
msgid "Only root can run %s.\\n"
msgstr "Kun root kan køre %s.\\n"

#: ../configsave.sh:23
msgid "usage: %s /media/sda?/%s\\n"
msgstr "brug: %s /media/sda?/%s\\n"

#: ../configsave.sh:24
msgid "Not a regular file: %s\\n"
msgstr "Ikke en almindelig fil: %s\\n"

#: ../configsave.sh:28
msgid "This script can work only from FWLive."
msgstr "Dette skript kan kun arbejde fra FWLive."

#: ../configsave.sh:40
msgid "Saving changes to %s... "
msgstr "Gemmer ændringer til %s... "

#: ../configsave.sh:45
msgid "Can't write config to %s\\n"
msgstr "Kan ikke skrive konfiguration til %s\\n"

#: ../configsave.sh:47 ../configsave.sh:58
msgid "done."
msgstr "gjort."

#: ../configsave.sh:55
msgid "Restoring changes from %s... "
msgstr "Gendannet ændringer fra %s... "

#: ../fileswap.sh:18
msgid "File Swap - create swap as a file in writable filesystem"
msgstr "Fil Swap - opretter swap som en fil i et skrivbart filsystem"

#: ../fileswap.sh:19
msgid "usage: %s [/media/partition/new_name.swap] [size MB]\\n"
msgstr "brug: %s [/media/partition/nyt_navn.swap] [størrelse MB]\\n"

#: ../fileswap.sh:29
msgid "you must use path in /media"
msgstr "du skal buge sti i /media"

#: ../fileswap.sh:33
msgid "creating empty %s\\n"
msgstr "opretter tom %s\\n"

#: ../fileswap.sh:37
msgid "setting up Linux swap area on %s\\n"
msgstr "opsætter et Linux swap område på %s\\n"

#: ../fileswap.sh:41
msgid "activating swap on %s\\n"
msgstr "aktiverer swap på %s\\n"

#: ../fpm2lzm.sh:37
msgid "usage: %s out.lzm pkg1 [ pkg2 [ ... ] ]\\n"
msgstr "brug: %s ud.lzm pkg1 [ pkg2 [ ... ] ]\\n"

#: ../rc.config.sh:30
msgid "Searching for a previous FWLive configuration file"
msgstr "Søger efter en foregående FWLive konfigurationsfil"

#: ../rc.config.sh:33 ../rc.config.sh:67
msgid "Found an FWLive config file."
msgstr "Fandt en FWLive konfigurationsfil."

#: ../rc.config.sh:34
msgid "Do you want to restore saved settings from %s?\\n"
msgstr "Ønsker du at gendanne gemte opsætning fra %s?\\n"

#: ../rc.config.sh:39 ../rc.config.sh:73
msgid "No previous FWLive config file found."
msgstr "Ingen foregående FWLive konfigurationsfil fundet."

#: ../rc.config.sh:48
msgid "enter=confirm"
msgstr "enter=bekræfte"

#: ../rc.config.sh:56
msgid "\\nTimed out. Never mind, skipped."
msgstr "\\nTidsudløbet. Glem det, skippet."

#. automatically restore/save settings in the case fwliveconf.lzm is found
#. in root directory of any partition. Only the first one is used.
#: ../rc.config.sh:64
msgid "Storing FWLive configuration file"
msgstr "Lagring af FWLive konfigurationsfil"

#: ../rc.config.sh:68
msgid "Save settings to %s?\\n"
msgstr "Gem indstillinger til %s?\\n"

#: ../rc.config.sh:74
msgid "Do you want to save settings?"
msgstr "Ønsker du at gemme indstillinger?"

#: ../rc.config.sh:75
msgid "Where do you want to save your settings? [example: /media/hda1]: "
msgstr "Hvor ønsker du at gemme dine indstillinger? [f.eks: /media/hda1]: "

#: ../rc.config.sh:81
msgid "No such directory."
msgstr "Et sådan filkatalog forefindes ikke."

#: ../rc.fsupd.sh:16
msgid "fstab updater daemon"
msgstr "fstab opdaterer daemon"

#: ../rc.fwlive.sh:16
msgid "\\e[01mWelcome to \\e[01;36mFwlive\\e[0m\\e[01m!\\e[0m"
msgstr "\\e[01mVelkommen til \\e[01;36mFwlive\\e[0m\\e[01m!\\e[0m"

#: ../rc.parse_cmdline.sh:17
msgid "Parsing /proc/cmdline"
msgstr "Analyserer /proc/cmdline"

#: ../parse_cmdline.sh:48
msgid "Welcome to FWLive %s by %s"
msgstr "Velkommen til FWLive %s af %s"

#: ../parse_cmdline.sh:52
msgid "The system is up and running now."
msgstr "Systemet er oppe og kører nu."

#: ../parse_cmdline.sh:53
msgid "Login as '%s', with password '%s' or"
msgstr "Log ind som '%s', med password '%s' eller"

#: ../parse_cmdline.sh:55
msgid "login as 'root', with password '%s', both without quotes, lowercase."
msgstr ""
"log ind som 'root', med password '%s', begge uden anførselstegn, små "
"bogstaver."

#: ../parse_cmdline.sh:56
msgid "After login, try the following commands:"
msgstr "Efter log ind, forsøg følgende komandoer:"

#: ../parse_cmdline.sh:57
msgid ""
"mc .... to start Midnight Commander (edit/copy/move/create/delete files)"
msgstr ""
"mc .... gå igang med Midnight Commander (redigere/kopiere/flytte/oprette/"
"slette filer)"

#: ../parse_cmdline.sh:58
msgid "configsave/configrestore  - or use - service config save/restore ..."
msgstr ""
"configsave/configrestore  - eller brug - service config save/restore ..."

#: ../parse_cmdline.sh:59
msgid "to save and restore all filesystem changes"
msgstr "at gemme og genskabe alle filsystemsændringer"

#: ../parse_cmdline.sh:60
msgid "fileswap .... to create special file for swapping RAM to your harddisk"
msgstr "fileswap .... at lave en speciel fil for swap RAM til din harddisk"

#: ../parse_cmdline.sh:61
msgid "make_disk .... to install FWLive, use make_disk script on FWLive CD"
msgstr ""
"make_disk .... at installere FWLive, brug make_disk script på FWLive CD"

#: ../parse_cmdline.sh:62
msgid ""
"xstart .... to start KDE with autodetected settings (upon failure vesa "
"driver will be used)"
msgstr ""
"xstart .... at starte KDE med automatisk sporingsindstillinger  (fejl sikker "
"vesa driver vil blive brugt)"

#: ../parse_cmdline.sh:63
msgid "setup .... a script for your own settings"
msgstr "setup .... et script for dine egne indstillinger "

#: ../parse_cmdline.sh:64
msgid ""
"When finished, use 'poweroff' or 'reboot' command and wait until it completes"
msgstr ""
"Når du er færdig, brug 'poweroff' eller 'reboot' komandoen og vent indtil "
"det er fuldført"
